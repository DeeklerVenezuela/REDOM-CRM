@model IEnumerable<RD.WEBAPI.Models.pago>

    @{
        ViewBag.Title = "Reporte 607";
    }
    <div class="wrapper">

        @Html.Partial("~/Views/Shared/_Header.cshtml")
        @Html.Partial("~/Views/Shared/_SidebarLeft.cshtml")
        <div class="wrapper">
            <div class="content-wrapper">
                <section class="content-header">
                    <h1>
                        Sistema de gestión
                        <small>Reportes</small>
                    </h1>
                    <ol class="breadcrumb">
                        <li><a href="#"><i class="fa fa-newspaper-o"></i> Home</a></li>
                        <li><a href="#">Reporte 607</a></li>
                    </ol>
                </section>
                <section class="content">
                    <div class="row">
                        <div class="col-xs-12">
                            <div class="box">
                                <div class="box-header">
                                    <h3 class="box-title">Reporte 607</h3>
                                    <button class="pull-right btn btn-success" onclick="List607()">Generar Reporte</button>
                                    <hr />
                                </div>
                                <table class="datatableUI table table-bordered table-striped">
                                    <thead>
                                        <tr>
                                            <th>
                                                RNC o Cedula
                                            </th>
                                            <th>
                                                Tipo de Identificación
                                            </th>
                                            <th>
                                                Número de Comprobante Fiscal
                                            </th>
                                            <th>
                                                Fecha de Comprobante
                                            </th>
                                            <th>
                                                Itbis Facturado
                                            </th>
                                            <th>
                                                Monto Facturado
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.rncedula)
                                                </td>
                                                <td>
                                                    1
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.ncomprobantef)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.fechaComprobante)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.itbisFacturado)
                                                </td>
                                                <td>
                                                    @Html.DisplayFor(modelItem => item.montofacturado)
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                </section><!-- /.content -->
            </div><!-- /.content-wrapper -->
        </div>
    </div>


    @*Import Sidebar Right*@
    @Html.Partial("~/Views/Shared/_SidebarRight.cshtml")
