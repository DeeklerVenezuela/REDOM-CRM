@model IEnumerable<RD.Data.Models.MembresiaVence>

@{
    ViewBag.Title = "Afiliaciones Vencidas";
}
<div class="wrapper">

    @Html.Partial("~/Views/Shared/_Header.cshtml")

    @*Import Sidebar Left*@
    @Html.Partial("~/Views/Shared/_SidebarLeft.cshtml")
    <div class="wrapper">
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Sistema de gestión
                    <small>Afiliaciones Vencidas</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-newspaper-o"></i> Home</a></li>
                    <li><a href="#">Afiliaciones Vencidas</a></li>
                </ol>
            </section>
            <section class="content">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title">Afiliaciones Vencidas</h3>
                                <hr />
                            </div><!-- /.box-header -->
                            @*<p>
                                @Html.ActionLink("Create New", "Create")
                            </p>*@
                            <div class="box-body">
                                <table class="datatableUI table table-bordered table-striped">
                                    <thead>
                                        <tr>
                                            <th>
                                                ID Afiliado
                                            </th>
                                            <th>
                                                Cédula
                                            </th>
                                            <th>
                                                Nombre
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.FechaInicio)
                                            </th>
                                            <th>
                                                @Html.DisplayNameFor(model => model.FechaFin)
                                            </th>                                            
                                            <th></th>
                                        </tr>
                                    </thead>

                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>
                                                @{var i = item.AfiliadoM.AfiliadoIndice.ToString();
                                                if (i.Length > 4)
                                                {
                                                    @Html.Raw("7777-2552-" + i.Substring(1, 4) + "-" + i.Substring(4, 4))
                                                }
                                                else
                                                {
                                                    @Html.DisplayFor(modelItem => item.AfiliadoM.AfiliadoIndice)
                                                }

                                                }
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.AfiliadoM.Cedula)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.AfiliadoM.Nombres)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FechaInicio)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.FechaFin)
                                            </td>
                                            @*<td>
       @{
            var Called = item.Called.ToString();
            if (Called == "False")
            {
                <p class="text-red">@Html.Raw("No LLamado")</p>
            }
            else
            {
                <p class="text-green">@Html.Raw("LLamado")</p>
            }


       }
    </td>*@
                                            <td>
                                                @*Html.ActionLink("Renovar","Edit", new { id = item.AfiliadoM.AfiliadoID }) |*@
                                                @Html.ActionLink("Renovar", "Edit", "Afiliados", new { id = item.AfiliadoM.AfiliadoID }, null)
                                                @*@Html.ActionLink("Details", "Details", new { id = item.MembresiaVenceID }) |
                                                @Html.ActionLink("Delete", "Delete", new { id = item.MembresiaVenceID })*@
                                            </td>
                                        </tr>
                                    }

                                </table>
                            </div>
                        </div><!-- /.box -->
                    </div><!-- /.col -->
                </div><!-- /.row -->
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->
                    </div>
                </div>


                @*Import Sidebar Right*@
                @Html.Partial("~/Views/Shared/_SidebarRight.cshtml")
