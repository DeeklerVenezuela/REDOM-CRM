@model RD.Data.Models.Prospecto

@{
    ViewBag.Title = "Prospectos";
}

<div class="wrapper">

    @Html.Partial("~/Views/Shared/_Header.cshtml")

    @*Import Sidebar Left*@
    @Html.Partial("~/Views/Shared/_SidebarLeft.cshtml")
    <div class="wrapper">
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Sistema de gestión
                    <small>Prospectos</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-newspaper-o"></i> Home</a></li>
                    <li><a href="#">Prospectos</a></li>
                </ol>
            </section>
            <section class="content">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="box">
                            <div class="box-header">
                                <h3 class="box-title">Prospectos Registrados</h3>
                                <hr />
                            </div><!-- /.box-header -->
                            <div class="box-body">

                                <div>
                                    <h4>Prospecto</h4>
                                    <hr />
                                    <dl class="dl-horizontal">
                                        <dt>
                                            @Html.DisplayNameFor(model => model.Nombres)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Nombres)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Apellido1)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Apellido1)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Apellido2)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Apellido2)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Cedula)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Cedula)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Respuesta)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Respuesta)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Fax)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Fax)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.FechaHora)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.FechaHora)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Comentario)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Comentario)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.FechaDeNacimiento)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.FechaDeNacimiento)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Email)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Email)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Activo)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Activo)
                                        </dd>

                                        <dt>
                                            @Html.DisplayNameFor(model => model.Ocupado)
                                        </dt>

                                        <dd>
                                            @Html.DisplayFor(model => model.Ocupado)
                                        </dd>

                                    </dl>
                                </div>
                                <p>
                                    @Html.ActionLink("Editar", "Editar", new { id = Model.ProspectoID }) |
                                    @Html.ActionLink("Volver al listado", "Index")
                                </p>
                            </div><!-- /.box-body -->
                        </div><!-- /.box -->
                    </div><!-- /.col -->
                </div><!-- /.row -->
            </section><!-- /.content -->
        </div><!-- /.content-wrapper -->
    </div>
</div>


@*Import Sidebar Right*@
@Html.Partial("~/Views/Shared/_SidebarRight.cshtml")