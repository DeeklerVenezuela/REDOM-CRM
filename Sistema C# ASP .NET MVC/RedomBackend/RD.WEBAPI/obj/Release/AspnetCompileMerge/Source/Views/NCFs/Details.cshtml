@model RD.Data.Models.NCF

@{
    ViewBag.Title = "NCF";
}

<div class="wrapper">
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Sistema de gestión
                <small>NCF</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-newspaper-o"></i> Home</a></li>
                <li><a href="#">NCF</a></li>
            </ol>
        </section>
        <section class="content">
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">Número de Comprobante Fiscal</h3>
                            <hr />
                        </div><!-- /.box-header -->
                        <div class="box-body">

                            <div>

                                <dl class="dl-horizontal">
                                    <dt>
                                        @Html.DisplayNameFor(model => model.Serie)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.Serie)
                                    </dd>

                                    <dt>
                                        @Html.DisplayNameFor(model => model.NumeradorInicial)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.NumeradorInicial)
                                    </dd>

                                    <dt>
                                        @Html.DisplayNameFor(model => model.NumeradorFinal)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.NumeradorFinal)
                                    </dd>

                                    <dt>
                                        @Html.DisplayNameFor(model => model.NumeradorActual)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.NumeradorActual)
                                    </dd>

                                    <dt>
                                        @Html.DisplayNameFor(model => model.IndicadorDeFinal)
                                    </dt>

                                    <dd>
                                        @Html.DisplayFor(model => model.IndicadorDeFinal)
                                    </dd>

                                </dl>
                            </div>
                            <p>
                                @Html.ActionLink("Editar", "Edit", new { id = Model.NCFID }) |
                                @Html.ActionLink("Volver al Listado", "Index")
                            </p>
                        </div><!-- /.box-body -->
                    </div><!-- /.box -->
                </div><!-- /.col -->
            </div><!-- /.row -->
        </section><!-- /.content -->
    </div><!-- /.content-wrapper -->
</div>
</div>


@*Import Sidebar Right*@
@Html.Partial("~/Views/Shared/_SidebarRight.cshtml")